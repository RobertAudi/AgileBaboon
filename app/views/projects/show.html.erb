<section id="project">
  <h3 class="heading"><%= @project.name %></h3>
  <p><%= @project.description %></p>

  <h4>Open Issues</h4>
  <ul>
    <% @project.issues.each do |issue| %>
      <li><%= issue.title %></li>
    <% end %>
  </ul>

  <h4>Users</h4>
  <ul>
  <% @project.users.each do |user| %>
    <li><%= user.username %></li>
  <% end %>
  </ul>
</section>
